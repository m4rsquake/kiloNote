{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"composite": false,
		"module": "ESNext",
		"target": "ES2020",
		"moduleResolution": "Node",
		"noEmit": true,
		"types": ["obsidian", "node"],
		"allowSyntheticDefaultImports": true,
		"useDefineForClassFields": false
	},
	"include": ["src/**/*.ts"],
	"exclude": ["dist", "node_modules"]
}
